-main ApplicationMain 
-cp /usr/local/lib/haxelib/openfl/3,3,9
-D openfl=3.3.9
-cp /usr/local/lib/haxelib/lime/2,6,9
-D lime=2.6.9
-cp src
-D openfl-next
-D tools=2.6.9
-D no-compilation
-D lime-opengl
-D openfl-html5
